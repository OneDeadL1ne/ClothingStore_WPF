<Window x:Class="ClothingStore.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClothingStore.Windows"
        mc:Ignorable="d"
        Title="MainWindow"
        xmlns:Icon="http://metro.mahapps.com/winfx/xaml/iconpacks"
        MinHeight="800"
        ResizeMode="CanResize"
        MinWidth="1250"
        WindowStartupLocation="CenterScreen"
        
        
        Icon="{Icon:MaterialImage Kind=TshirtCrew}"
        >
    <Grid Background="White">
        
          <Frame x:Name="windowFrame"
             NavigationUIVisibility="Hidden"
                 
                 
             />

          <Frame x:Name="AuthorizationFrame" 
             NavigationUIVisibility="Hidden"
                 
                 Navigating="AuthorizationFrame_Navigating"/>



       
       
    </Grid>
</Window>
